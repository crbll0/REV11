<odoo>
  <data>
    <record model="ir.ui.view" id="inherit_real_estate_property_form_view">
      <field name="name">Property</field>
      <field name="model">real.estate.property</field>
      <field name="inherit_id" ref="real_estate.property_form_view"/>
      <field name="arch" type="xml">
        <xpath expr="//sheet/div[@class='oe_title']" position="before">
          <div class="oe_button_box" name="button_box">
            <button class="oe_stat_button" name="website_publish_button"
                    type="object" icon="fa-globe">
              <field name="website_published" widget="website_button"/>
            </button>
          </div>
        </xpath>
        <xpath expr="//notebook" position="inside">
          <page string="Website Desc.">
            <field name="website_description"/>
          </page>
        </xpath>
      </field>
    </record>


  </data>
</odoo>