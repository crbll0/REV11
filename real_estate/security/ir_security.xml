<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="module_real_estate_category" model="ir.module.category">
    <field name="name">Real Estate</field>
    <field name="sequence">20</field>
  </record>

  <record id="group_real_estate_user" model="res.groups">
    <field name="name">Officer</field>
    <field name="category_id" ref="module_real_estate_category"/>
    <field name="implied_ids"
           eval="[(6, 0, [ref('base.group_private_addresses'), ref('base.group_user')])]"/>
    <!--<field name="comment"></field>-->
  </record>

  <record id="group_real_estate_manager" model="res.groups">
    <field name="name">Manager</field>
    <!--<field name="comment"></field>-->
    <field name="category_id" ref="module_real_estate_category"/>
    <field name="implied_ids" eval="[(4, ref('group_real_estate_user'))]"/>
    <field name="users"
           eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
  </record>
</odoo>